/* pages/shopCar/shopCar.wxss */
.wrap{
  height:130rpx;
  width:100%;
  position:relative;
  top:0;
  left:0;
  overflow: hidden;
}
.wrap-delete{
  width:100rpx;
  height:130rpx;
  position:absolute;
  right:0;
  top:0;
  background: red;
  font-size: 30rpx;
  color:#fff;
  line-height:130rpx;
  text-align: center;
  z-index:0;
}
.wrap-main{
  position:relative;
  top:0;
  left:0;
  z-index:100;
  height:130rpx;
  display: flex;
  align-items: center; 
  background: #fff;
}
.wrap-img{
  height:100rpx;
  width:100rpx;
  margin:0 20rpx;
}
.wrap-text{
  font-size:28rpx;
  color:black;
}